<?xml version="1.0" encoding="UTF-8"?>
<widget version="0.1">
    <title xml:lang="ko">서버상태 위젯</title>
    <description xml:lang="ko">서버상태를 표시해주는 위젯입니다.</description>
    <version>0.1</version>
    <date>2013-04-10</date>
    <author email_address="cs8minty@gmail.com" link="http://sbira.com">
        <name xml:lang="ko">스비라 (SBIRA)</name>
    </author>
    <extra_vars>
		<var id="server_ip" type="text">
			<name xml:lang="ko">서버 주소</name>
		</var>

		<var id="query_port" type="text">
			<name xml:lang="ko">쿼리 포트</name>
			<description xml:lang="ko">기본값 (25566)</description>
		</var>
		
		<var id="avatar_site" type="select">
			<name xml:lang="ko">아바타 제공 서버</name>
			<options>
			  <value>Minotar</value>
			  <name xml:lang="ko">Minotar</name>
			</options>
			<options>
			  <value>Empire Minecraft</value>
			  <name xml:lang="ko">Empire Minecraft</name>
			</options>
			<options>
			  <value>SBI CRAFT</value>
			  <name xml:lang="ko">SBI CRAFT</name>
			</options>
		</var>
	</extra_vars>
</widget>
